      }
    ],
    "syspass": [
      {
        "apikey": "",
        "apikeypassphrase": "",
        "count": 1,
        "endpoint": ""
      },
      {
        "apikey": "",
        "apikeypassphrase": "",
        "count": 1,
        "endpoint": ""
      }
    ]
  }
