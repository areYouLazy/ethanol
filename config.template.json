{
  "ethanol": {
    "client": {
      "proxy": {
        "address": "127.0.0.1",
        "authenticate": false,
        "cacertificate": "",
        "enable": false,
        "password": "",
        "port": 8080,
        "protocol": "http",
        "username": ""
      },
      "useragent": "ethanol/1.0"
    },
    "server": {
      "address": "0.0.0.0",
      "forwardedbyclientip": false,
      "idletimeout": 60,
      "pluginsfolder": "/etc/ethanol/search_providers/",
      "port": 8888,
      "readtimeout": 15,
      "remoteipheaders": [
        "X-Forwarded-For",
        "X-Real-IP"
      ],
      "security": {
        "headers": {
          "cachecontrol": "no-cache",
          "csp": "default-src * 'unsafe-inline' 'unsafe-eval'",
          "hsts": "maxage=31536000; includeSubDomains",
          "xcontenttypeoptions": "nosniff",
          "xframeoptions": "SAMEORIGIN",
          "xssprotection": "1; mode=block"
        },
        "secure": true
      },
      "signature": {
        "server": "ethanol",
        "xpoweredby": "gin-gonic"
      },
      "truestedproxies": [
        "localhost",
        "127.0.0.1"
      ],
      "websocket": {
        "origins": [
          "localhost",
          "127.0.0.1"
        ],
        "readbuffersize": 1024,
        "writebuffersize": 1024
      },
      "writetimeout": 15
    }
  },
  "plugins": {
    "checkmk": [
      {
        "endpoint": "",
        "password": "",
        "username": ""
      },
